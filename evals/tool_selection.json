{
  "name": "MediaWiki MCP Tool Selection Evals",
  "version": "1.0.0",
  "description": "Evaluation cases for testing LLM tool selection accuracy",
  "tests": [
    {
      "id": "search-001",
      "category": "search",
      "input": "find all pages about authentication",
      "expected_tool": "mediawiki_search",
      "expected_args": {"query": "authentication"},
      "not_tools": ["mediawiki_search_in_page", "mediawiki_get_page"]
    },
    {
      "id": "search-002",
      "category": "search",
      "input": "where is the API documentation?",
      "expected_tool": "mediawiki_search",
      "expected_args": {"query": "API documentation"},
      "not_tools": ["mediawiki_get_page"]
    },
    {
      "id": "search-003",
      "category": "search",
      "input": "does the Installation Guide mention Docker?",
      "expected_tool": "mediawiki_search_in_page",
      "expected_args": {"title": "Installation Guide", "query": "Docker"},
      "not_tools": ["mediawiki_search", "mediawiki_get_page"]
    },
    {
      "id": "search-004",
      "category": "search",
      "input": "find mentions of timeout on the Configuration page",
      "expected_tool": "mediawiki_search_in_page",
      "expected_args": {"title": "Configuration", "query": "timeout"},
      "not_tools": ["mediawiki_search"]
    },
    {
      "id": "search-005",
      "category": "search",
      "input": "search for deprecated features in the API Reference",
      "expected_tool": "mediawiki_search_in_page",
      "expected_args": {"title": "API Reference", "query": "deprecated"},
      "not_tools": ["mediawiki_search"]
    },
    {
      "id": "read-001",
      "category": "read",
      "input": "show me the Getting Started page",
      "expected_tool": "mediawiki_get_page",
      "expected_args": {"title": "Getting Started"},
      "not_tools": ["mediawiki_search", "mediawiki_list_pages"]
    },
    {
      "id": "read-002",
      "category": "read",
      "input": "what's on the Main Page?",
      "expected_tool": "mediawiki_get_page",
      "expected_args": {"title": "Main Page"},
      "not_tools": ["mediawiki_search"]
    },
    {
      "id": "read-003",
      "category": "read",
      "input": "list all wiki pages",
      "expected_tool": "mediawiki_list_pages",
      "expected_args": {},
      "not_tools": ["mediawiki_get_page", "mediawiki_search"]
    },
    {
      "id": "read-004",
      "category": "read",
      "input": "show me all pages starting with API",
      "expected_tool": "mediawiki_list_pages",
      "expected_args": {"prefix": "API"},
      "not_tools": ["mediawiki_search"]
    },
    {
      "id": "read-005",
      "category": "read",
      "input": "what sections does the User Guide have?",
      "expected_tool": "mediawiki_get_sections",
      "expected_args": {"title": "User Guide"},
      "not_tools": ["mediawiki_get_page"]
    },
    {
      "id": "read-006",
      "category": "read",
      "input": "show me the Installation section of the Quick Start guide",
      "expected_tool": "mediawiki_get_sections",
      "expected_args": {"title": "Quick Start"},
      "not_tools": ["mediawiki_get_page"]
    },
    {
      "id": "read-007",
      "category": "read",
      "input": "when was the Release Notes page last edited?",
      "expected_tool": "mediawiki_get_page_info",
      "expected_args": {"title": "Release Notes"},
      "not_tools": ["mediawiki_get_page", "mediawiki_get_revisions"]
    },
    {
      "id": "resolve-001",
      "category": "search",
      "input": "I tried to find 'module overview' but got page not found",
      "expected_tool": "mediawiki_resolve_title",
      "expected_args": {"title": "module overview"},
      "not_tools": ["mediawiki_search", "mediawiki_get_page"]
    },
    {
      "id": "resolve-002",
      "category": "search",
      "input": "the page Configration doesn't exist, maybe I spelled it wrong",
      "expected_tool": "mediawiki_resolve_title",
      "expected_args": {"title": "Configration"},
      "not_tools": ["mediawiki_search"]
    },
    {
      "id": "history-001",
      "category": "history",
      "input": "who edited the Privacy Policy page?",
      "expected_tool": "mediawiki_get_revisions",
      "expected_args": {"title": "Privacy Policy"},
      "not_tools": ["mediawiki_get_page_info", "mediawiki_get_user_contributions"]
    },
    {
      "id": "history-002",
      "category": "history",
      "input": "show the edit history of the Main Page",
      "expected_tool": "mediawiki_get_revisions",
      "expected_args": {"title": "Main Page"},
      "not_tools": ["mediawiki_get_recent_changes"]
    },
    {
      "id": "history-003",
      "category": "history",
      "input": "what pages did john.smith edit?",
      "expected_tool": "mediawiki_get_user_contributions",
      "expected_args": {"user": "john.smith"},
      "not_tools": ["mediawiki_get_revisions", "mediawiki_get_recent_changes"]
    },
    {
      "id": "history-004",
      "category": "history",
      "input": "show recent activity on the wiki",
      "expected_tool": "mediawiki_get_recent_changes",
      "expected_args": {},
      "not_tools": ["mediawiki_get_revisions"]
    },
    {
      "id": "history-005",
      "category": "history",
      "input": "compare the current version with the previous version of the FAQ",
      "expected_tool": "mediawiki_compare_revisions",
      "expected_args": {"from_title": "FAQ"},
      "not_tools": ["mediawiki_get_revisions"]
    },
    {
      "id": "category-001",
      "category": "categories",
      "input": "what categories exist on the wiki?",
      "expected_tool": "mediawiki_list_categories",
      "expected_args": {},
      "not_tools": ["mediawiki_get_category_members"]
    },
    {
      "id": "category-002",
      "category": "categories",
      "input": "show all pages in the Documentation category",
      "expected_tool": "mediawiki_get_category_members",
      "expected_args": {"category": "Documentation"},
      "not_tools": ["mediawiki_list_categories", "mediawiki_search"]
    },
    {
      "id": "links-001",
      "category": "links",
      "input": "what pages link to the API Reference?",
      "expected_tool": "mediawiki_get_backlinks",
      "expected_args": {"title": "API Reference"},
      "not_tools": ["mediawiki_get_external_links", "mediawiki_get_related"]
    },
    {
      "id": "links-002",
      "category": "links",
      "input": "show external URLs on the Resources page",
      "expected_tool": "mediawiki_get_external_links",
      "expected_args": {"title": "Resources"},
      "not_tools": ["mediawiki_get_backlinks", "mediawiki_check_links"]
    },
    {
      "id": "links-003",
      "category": "links",
      "input": "check if these links are working: https://example.com, https://test.org",
      "expected_tool": "mediawiki_check_links",
      "expected_args": {"urls": ["https://example.com", "https://test.org"]},
      "not_tools": ["mediawiki_get_external_links", "mediawiki_find_broken_internal_links"]
    },
    {
      "id": "links-004",
      "category": "links",
      "input": "find broken internal links on the Main Page",
      "expected_tool": "mediawiki_find_broken_internal_links",
      "expected_args": {"pages": ["Main Page"]},
      "not_tools": ["mediawiki_check_links", "mediawiki_get_external_links"]
    },
    {
      "id": "links-005",
      "category": "links",
      "input": "find pages that have no incoming links",
      "expected_tool": "mediawiki_find_orphaned_pages",
      "expected_args": {},
      "not_tools": ["mediawiki_get_backlinks", "mediawiki_find_broken_internal_links"]
    },
    {
      "id": "quality-001",
      "category": "quality",
      "input": "check if pages use correct brand terminology",
      "expected_tool": "mediawiki_check_terminology",
      "expected_args": {},
      "not_tools": ["mediawiki_search"]
    },
    {
      "id": "quality-002",
      "category": "quality",
      "input": "which pages are missing translations in German and French?",
      "expected_tool": "mediawiki_check_translations",
      "expected_args": {"languages": ["de", "fr"]},
      "not_tools": ["mediawiki_search", "mediawiki_list_pages"]
    },
    {
      "id": "quality-003",
      "category": "quality",
      "input": "run a health check on the wiki",
      "expected_tool": "mediawiki_audit",
      "expected_args": {},
      "not_tools": ["mediawiki_check_terminology", "mediawiki_find_broken_internal_links"]
    },
    {
      "id": "discovery-001",
      "category": "discovery",
      "input": "find pages similar to the Installation Guide",
      "expected_tool": "mediawiki_find_similar_pages",
      "expected_args": {"page": "Installation Guide"},
      "not_tools": ["mediawiki_search", "mediawiki_get_related"]
    },
    {
      "id": "discovery-002",
      "category": "discovery",
      "input": "compare how different pages describe the timeout setting",
      "expected_tool": "mediawiki_compare_topic",
      "expected_args": {"topic": "timeout"},
      "not_tools": ["mediawiki_search", "mediawiki_compare_revisions"]
    },
    {
      "id": "write-001",
      "category": "write",
      "input": "replace 'Public 360' with 'Public 360°' on the Product page",
      "expected_tool": "mediawiki_find_replace",
      "expected_args": {"title": "Product", "find": "Public 360", "replace": "Public 360°"},
      "not_tools": ["mediawiki_edit_page", "mediawiki_bulk_replace"]
    },
    {
      "id": "write-002",
      "category": "write",
      "input": "strike out John Smith's name on the Team page",
      "expected_tool": "mediawiki_apply_formatting",
      "expected_args": {"title": "Team", "text": "John Smith", "format": "strikethrough"},
      "not_tools": ["mediawiki_find_replace", "mediawiki_edit_page"]
    },
    {
      "id": "write-003",
      "category": "write",
      "input": "make the word Important bold on the Guidelines page",
      "expected_tool": "mediawiki_apply_formatting",
      "expected_args": {"title": "Guidelines", "text": "Important", "format": "bold"},
      "not_tools": ["mediawiki_find_replace"]
    },
    {
      "id": "write-004",
      "category": "write",
      "input": "update the company name from Acme to Acme Inc across all documentation pages",
      "expected_tool": "mediawiki_bulk_replace",
      "expected_args": {"find": "Acme", "replace": "Acme Inc", "category": "Documentation"},
      "not_tools": ["mediawiki_find_replace", "mediawiki_edit_page"]
    },
    {
      "id": "write-005",
      "category": "write",
      "input": "create a new page called Release Notes 2025",
      "expected_tool": "mediawiki_edit_page",
      "expected_args": {"title": "Release Notes 2025"},
      "not_tools": ["mediawiki_find_replace"]
    },
    {
      "id": "write-006",
      "category": "write",
      "input": "completely rewrite the About page with new content",
      "expected_tool": "mediawiki_edit_page",
      "expected_args": {"title": "About"},
      "not_tools": ["mediawiki_find_replace", "mediawiki_apply_formatting"]
    }
  ]
}
